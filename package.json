{
  "name": "vnd-nodejs-backend-template",
  "version": "1.0.0",
  "engines": {
    "node": ">=14.17.1"
  },
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "export NODE_ENV=development && TZ='Asia/Ho_Chi_Minh' && ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "export NODE_ENV=development && concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "export NODE_ENV=production && node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "preinstall": "npx npm-force-resolutions",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate --config ./ormconfig.js --name",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run --config ./ormconfig.js",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert --config ./ormconfig.js"
  },
  "resolutions": {
    "**/**/fs-capacitor": "^6.2.0",
    "**/graphql-upload": "^11.0.0"
  },
  "dependencies": {
    "@nestjs/common": "7.6.18",
    "@nestjs/core": "7.6.18",
    "@nestjs/graphql": "7.11.0",
    "@nestjs/jwt": "6.1.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/microservices": "^6.11.8",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.5.5",
    "moment": "^2.29.1",
    "jsonwebtoken": "^8.5.1",
    "@nestjs/platform-express": "7.6.18",
    "@nestjs/swagger": "4.8.1",
    "@nestjs/typeorm": "^7.1.0",
    "@types/express": "^4.17.7",
    "apollo-server-express": "^2.25.2",
    "aws-sdk": "^2.799.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.12.2",
    "dateformat": "^4.5.1",
    "dotenv": "^8.6.0",
    "easygraphql-format-error": "^0.0.3",
    "fast-sha256": "^1.3.0",
    "fs-extra": "^9.1.0",
    "graphql": "^15.5.1",
    "graphql-2-json-schema": "^0.6.0",
    "graphql-to-json-converter": "^1.2.0",
    "graphql-tools": "^6.2.6",
    "graphql-upload": "^12.0.0",
    "helmet": "^4.1.0",
    "sharp": "^0.28.3",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "nestjs-fcm": "^0.1.0",
    "nestjs-stripe": "^0.4.0",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "stripe": "^8.156.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "typescript": "4.3.4"
  },
  "devDependencies": {
    "@golevelup/nestjs-testing": "^0.1.2",
    "@nestjs/testing": "7.6.18",
    "@types/bcryptjs": "^2.4.2",
    "@types/crypto-js": "^4.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.10",
    "@types/node": "14.17.3",
    "@types/passport": "^1.0.4",
    "@types/sharp": "^0.28.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "concurrently": "^5.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "wait-on": "^5.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
